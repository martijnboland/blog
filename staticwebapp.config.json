{
  "navigationFallback": {
    "rewrite": "/api/legacyredirect"
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "routes": [
      {
          "route": "/index.html",
          "redirect": "/martijn"
      },
      {
          "route": "/",
          "redirect": "/martijn"
      }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/martijn/404.html",
      "statusCode": 404
    }
  }
}